          "type":"checkbox"
      },
      {
          "name":"course_type",
          "options":["all","is_manager_only", "is_mandatory","is_linear","invitation_only"],
          "type":"checkbox"
      }
     ],
     "orgs":["asia","americas","europe"],
     "homeFilters":["status","tag","course_type","org"],
     "createSteps":[
      ["course_name","org","course_number","course_session"],
      ["course_pacing","effort","course_dates","language","course_type"],
      ["tag","onboarding","course_image","description","course_goals","course_map","teacher_name","teacher_image","teacher_email"],
      ["course_settings","course_grade","course_downloads"]
     ]
