              "name":"course_name",
              "required":true,
              "type":"text"
          },
          {
              "name":"org",
              "required":true,
              "type":"select",
              "options":["asia","americas","europe"]
          },
          {
              "name":"course_number",
              "required":true,
              "type":"text"
          },
          {
              "name":"course_session",
              "required":true,
              "type":"text"
          },
          {
              "name":"course_pacing",
              "type":"radio",
              "options":[
                  "instructor_paced",
                  "self_paced"
              ]
          },
          {
              "name":"effort",
              "type":"time"
          },
          {
              "name":"course_dates",
              "type":"date"
  
          },
          {
              "name":"course_type",
              "type":"checkBox",
              "options":[
                  "invitation_only",
                  "is_manager_only",
                  "is_mandatory",
                  "is_linear"
              ]
          },
          {
              "name":"tag",
              "type":"select",
              "options":[
                  "computer science",
                  "finance",
                  "management",
                  "desktop",
                  "design"
              ]
          },
          {
              "name":"onboarding",
              "type":"select",
              "options":[
                  "new starter"
              ]
          },
          {
              "name":"course_image",
              "type":"imageUpload"
          },
          {
              "name":"description",
              "type":"textField"
          },
          {
              "name":"course_map",
              "type":"jsonEditor"
          },
          {
              "name":"teacher_name",
              "required":false,
              "type":"text"
          },
          {
              "name":"teacher_image",
              "hideTitle":true,
              "type":"imageUpload"
          },
          {
              "name":"teacher_email",
              "required":false,
              "hideTitle":true,
              "type":"text"
          },
          {
              "name":"course_settings",
              "type":"checkBox",
              "options":[
                  "is_course_graded",
                  "has_menu"
              ]
          },
          {
              "name":"course_grade",
              "type":"grade"
          },
          {
              "name":"course_downloads",
              "type":"multipleUploads"
          }
